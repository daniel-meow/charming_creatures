<div class="container mt-4 mb-4">
  <h1>All Articles</h1>
  <div class="articles">
    <% @articles.each do |article| %>
    <div class="article">
      <div class="article-img"></div>
      <div class="article-content">
        <h2><%= article.title  %></h2>
        <p>posted by: <%= article.club.name  %></p>
        <p>Description <%= article.description  %></p>
      </div>
    </div>
    <% end %>
  </div>
</div>